{
  "env": {
    "es2022": true,
    "browser": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@babel"],
  "rules": {
    "no-console": "off",
    "prettier/prettier": "error",
    "import/no-import-module-exports": "off"
  }
}
