{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "parser": "@babel/eslint-parser",
  "plugins": ["babel"],
  "rules": {
    "no-console": "off",
    "prettier/prettier": "error"
  }
}
